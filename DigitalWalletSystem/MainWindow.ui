        const requestMoneyBtn = document.getElementById('requestMoneyBtn');
        const viewRequestsBtn = document.getElementById('viewRequestsBtn');
        const sendMoneyModal = document.getElementById('sendMoneyModal');
        const requestMoneyModal = document.getElementById('requestMoneyModal');
        const viewRequestsModal = document.getElementById('viewRequestsModal');
        const modalCloseButtons = document.querySelectorAll('.modal-close, .modal-close-btn');
        
        sendMoneyBtn.addEventListener('click', () =&gt; {
            sendMoneyModal.classList.add('active');
        });
        
        requestMoneyBtn.addEventListener('click', () =&gt; {
            requestMoneyModal.classList.add('active');
        });
        
        viewRequestsBtn.addEventListener('click', () =&gt; {
            viewRequestsModal.classList.add('active');
        });
        
        modalCloseButtons.forEach(button =&gt; {
            button.addEventListener('click', () =&gt; {
                sendMoneyModal.classList.remove('active');
                requestMoneyModal.classList.remove('active');
                viewRequestsModal.classList.remove('active');
            });
        });
        
        // Close modals when clicking outside
        window.addEventListener('click', (e) =&gt; {
            if (e.target === sendMoneyModal) {
                sendMoneyModal.classList.remove('active');
            }
            if (e.target === requestMoneyModal) {
                requestMoneyModal.classList.remove('active');
            }
            if (e.target === viewRequestsModal) {
                viewRequestsModal.classList.remove('active');
            }
        });
